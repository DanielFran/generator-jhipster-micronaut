{
  "name": "generator-jhipster-micronaut",
  "version": "0.8.0",
  "description": "A Micronaut application generator for JHipster",
  "keywords": [
    "jhipster",
    "jhipster-7",
    "jhipster-blueprint",
    "Micronaut",
    "yeoman-generator"
  ],
  "homepage": "https://github.com/jhipster/generator-jhipster-micronaut",
  "author": {
    "name": "Sendil Kumar N",
    "email": "sendilkumarn@live.com",
    "url": "https://sendilkumarn.com"
  },
  "bin": {
    "mhipster": "./cli/mhipster.js"
  },
  "files": [
    "cli",
    "generators"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhipster/generator-jhipster-micronaut.git"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "commander": "4.1.1",
    "didyoumean": "1.2.1",
    "ejs": "3.1.6",
    "faker": "5.4.0",
    "generator-jhipster": "7.0.0",
    "jhipster-core": "7.3.4",
    "glob": "7.1.6",
    "insight": "0.10.3",
    "js-yaml": "3.14.0",
    "lodash": "4.17.21",
    "meow": "6.1.0",
    "mkdirp": "1.0.4",
    "pluralize": "8.0.0",
    "prettier": "2.2.1",
    "prettier-plugin-java": "1.0.2",
    "randexp": "0.5.3",
    "semver": "7.3.4",
    "shelljs": "0.8.4",
    "tabtab": "2.2.2",
    "yeoman-environment": "2.10.3",
    "yeoman-generator": "4.12.0"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "ejs-lint": "1.1.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "fs-extra": "9.1.0",
    "mocha": "7.1.2",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.6.0"
  },
  "scripts": {
    "ejs-lint": "npm run test:unit -- test/ejslint.js",
    "eslint": "eslint .",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "lint": "npm run eslint && npm run ejs-lint",
    "lint-fix": "npm run prettier:format && npm run eslint -- --fix",
    "pretest": "npm run lint",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release",
    "test": "npm run test:unit -- test/*.spec.js",
    "test:unit": "mocha --timeout 20000 --slow 0 --reporter spec"
  },
  "resolutions": {
    "inquirer": "6.3.1"
  },
  "engines": {
    "node": ">=10.20.1",
    "npm": ">=6.14.4"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jhipster/generator-jhipster-micronaut/issues"
  }
}
